{
    "name": "horizon-site",
    "version": "1.0.0",
    "description": "Front-end of horizon-efrei.fr",
    "author": "Horizon Front-end Dev. Team",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "lint": "eslint './src/**/*.{js,vue}'",
        "lint:fix": "npm run lint -- --fix --debug",
        "prettier": "prettier './src/**/*.{js,vue}' --check",
        "prettier:fix": "prettier './src/**/*.{js,vue}' --write",
        "stylelint": "stylelint './src/**/*.{vue,css,scss}'",
        "stylelint:fix": "stylelint './src/**/*.{vue,css,scss}' --fix",
        "format": "npm run lint:fix && npm run stylelint:fix && npm run prettier:fix",
        "format:soft": "npm run lint && npm run stylelint && npm run prettier",
        "build": "vite build",
        "serve": "vite preview"
    },
    "dependencies": {
        "@vuelidate/core": "2.0.0-alpha.34",
        "@vuelidate/validators": "2.0.0-alpha.26",
        "@vueuse/core": "8.0.0",
        "axios": "0.25.0",
        "lodash": "4.17.21",
        "mitt": "3.0.0",
        "nanoid": "3.2.0",
        "pinia": "2.0.11",
        "typesense-instantsearch-adapter": "2.2.0",
        "url-join": "4.0.1",
        "vditor": "^3.8.12",
        "vue": "3.2.30",
        "vue-avatar-cropper": "6.0.9",
        "vue-i18n": "9.2.0-beta.30",
        "vue-image-crop-upload": "3.0.3",
        "vue-instantsearch": "4.3.2",
        "vue-router": "4.0.12",
        "vue3-cookies": "1.0.6",
        "vue3-popper": "1.4.1",
        "vuex": "4.0.2"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "0.4.0",
        "@tailwindcss/forms": "0.4.0",
        "@tailwindcss/line-clamp": "0.3.1",
        "@tailwindcss/typography": "0.5.1",
        "@types/tailwindcss": "3.0.7",
        "@vitejs/plugin-vue": "2.1.0",
        "autoprefixer": "10.4.2",
        "eslint": "8.10.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-tailwindcss": "3.5.0",
        "eslint-plugin-vue": "8.5.0",
        "postcss-html": "1.3.0",
        "prettier": "2.5.1",
        "sass": "1.49.7",
        "sass-loader": "12.4.0",
        "stylelint": "14.4.0",
        "stylelint-config-css-modules": "4.0.1",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-recess-order": "3.0.0",
        "stylelint-config-recommended": "^7.0.0",
        "stylelint-config-recommended-scss": "^5.0.2",
        "stylelint-config-recommended-vue": "1.1.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-config-standard-scss": "3.0.0",
        "tailwindcss": "3.0.19",
        "vite": "2.7.13"
    }
}
